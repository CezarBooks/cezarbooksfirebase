      "jsx": "react",
      "module": "esnext",
      "moduleResolution": "node",
      "lib": ["dom", "esnext"],
      "strict": true,
      "strictNullChecks": false,
      "sourceMap": true,
      "target": "esnext",
      "outDir": "build"
